{
  "name": "webapp-cdeals",
  "version": "1.0.0",
  "description": "Web App for Cdeals",
  "main": "app.js",
  "engines": {
    "node": ">=10.16.x",
    "npm": ">=6.4.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf ./.next && next build",
    "sls:offline": "npm run sls:build && serverless offline start",
    "sls:deploy": "npm run sls:build && serverless deploy",
    "sls:build": "node ./serverless/build.js",
    "start": "npm run build && next start",
    "re-init": "rm -rf ./package-lock.json ./node_modules ./.babel-cache; npm install",
    "test": "jest",
    "test:watch": "jest --no-cache --watchAll",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "storybook": "start-storybook  -s ./public -p 6006",
    "storybook:export": "rm -rf ./storybook && build-storybook -o storybook",
    "es-lint": "eslint . --ext .jsx,.js",
    "es-lint:fix": "eslint . --ext .jsx,.js --fix",
    "prettier": "prettier  --check \"**/*.{jsx,js,json}\"",
    "prettier:fix": "prettier --check --write \"**/*.{jsx,js,json}\" ",
    "stylelint": "stylelint \"./src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint --fix \"./src/**/*.{css,scss}\"",
    "lint": "npm run es-lint && npm run prettier && npm run stylelint",
    "lint:fix": "npm run es-lint:fix && npm run prettier:fix && npm run stylelint:fix",
    "pre-push:validation": "npm run test:coverage && npm run storybook:export",
    "pipeline:validation": "npm run lint && npm run test:coverage && npm run build"
  },
  "pre-commit": [
    "lint"
  ],
  "pre-push": [
    "pre-push:validation"
  ],
  "browserslist": [
    "> 0.25%, not dead"
  ],
  "author": "Jebarin <jebarin.johnrose@nutechnologyinc.com>",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.3",
    "@contentful/rich-text-types": "^14.1.2",
    "@svgr/webpack": "^5.5.0",
    "chart.js": "^3.6.2",
    "contentful": "^8.4.0",
    "css-loader": "^5.2.6",
    "exec-sh": "^0.4.0",
    "final-form": "^4.20.2",
    "gb-tracker-client": "^5.0.0",
    "html-react-parser": "^1.2.7",
    "intersection-observer": "^0.12.0",
    "next": "^11.0.1",
    "next-redux-wrapper": "^6.0.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.3",
    "react-html-parser": "^2.0.2",
    "react-image-gallery": "^1.2.6",
    "react-inner-image-zoom": "^2.0.2",
    "react-input-mask": "^2.0.4",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.2",
    "react-simple-snackbar": "^1.1.11",
    "react-tooltip": "^4.2.21",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/react": "^6.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.20.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.3.2",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "redux-devtools-extension": "^2.13.8",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.0.0",
    "serverless": "^2.61.0",
    "serverless-express": "^2.0.12",
    "serverless-offline": "^6.9.0",
    "serverless-s3-sync": "^1.17.1",
    "stylelint": "^13.10.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0"
  }
}
